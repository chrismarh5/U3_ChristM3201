% Indica esta funcion  
pkg load symbolic
syms t;
int (t)
ans = (sym)
& Con el fin de llegar a este resultado= f(g)= gt

% Indica esta funcion 
pkg load symbolic
syms x;
int (x^3)
ans = (sym)
% Con el fin de llegar a este resultado= f(x)= x^4/4

% Indica esta funcion 
pkg load symbolic
syms x;
int (5*x)
ans = (sym)
& Con el fin de llegar a este resultado= f(x)= 5x

% Indica esta funcion 
pkg load symbolic
syms x;
int (8*x^2-3*x^3)
ans = (sym)
& Con el fin de llegar a este resultado= f(x)= 8x^3/3-3x^4

% Indica esta funcion 
pkg load symbolic
syms x;
int (2*e^x + 4^x)
ans = (sym)
& Con el fin de llegar a este resultado= f(x)= 2*e^x + 2(2x^-1/ln(2)

